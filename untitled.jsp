			});
		} else {
			$(document).ready(function() {
				$('#loginGoogle').attr('disabled', true);
				$('#loginGoogle').attr('title', 'Failed to init Google API.');
			});
		}					
		
		function showLoading(show) {
			if (show) {
				var body = $(document).find("body");
				var bW = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
				var bH = (window.innerHeight || document.documentElement.clientHeight ||document.body.clientHeight)+30;
				var content = $(".mask-content");
				var cW = content.width();
				var cH = content.height()+30;
				$(".starting-mask").css({width: bW, height: bH, visibility: "visible"});
				content.css({position: "fixed", left: ((bW-cW)/2)+"px", top: ((bH-cH)/2)+"px"});
			} else {
				$(".starting-mask").css({visibility: "hidden"});
			}
		}

  	function endsWith(string, check) {
	    var index = string.lastIndexOf(check);
	    return index==string.length-check.length;
	  }
  	
		function loginVPository(workspace, workspaceId, data) {
			data.ajax = true;
			return $.ajax({
				url: 'https://' + workspace + '/LoginServlet',
				type: 'POST',
				xhrFields: {
					withCredentials: true
				},
				data: data,
				success: function(data, textStatus, jqXHR) {
					showLoading(false);
					sessionStorage.removeItem('OnlineClient.baseUrl');
					if (data=="already login") {
						window.location = "member.jsp";
						return;
					}
					if (data.success) {
						Cookies.set('workspace', workspace, { expires: 365, secure: true });
						Cookies.set('workspaceId', workspaceId, { expires: 365, secure: true });
						Cookies.set('vponline-session', JSON.stringify({domain: workspace}), { domain: '.visual-paradigm.com', secure: true });
						if (data.redirectPage) {
							
							Cookies.set('memberPage', data.redirectPage, {secure: true});
							
							if (endsWith(window.location.pathname, '/') || endsWith(window.location.pathname, '/login.jsp')) {
								window.location.assign(data.redirectPage + window.location.hash);
							} else {
								window.location.reload();
							}
							
						}
					} else {
						if (typeof showError === 'function') {
							if (data.errorType === 'passwordExpired'/*LoginError.ErrorType.name*/) {
								showError($('#email'));
								$("#reset-password-error-link").text('Your password has expired. Click here to reset your password.');
								$("#reset-password-error-link").css('display', 'block');
							}
							else {
								showError($('#email'), data.error ? data.error : data);
								$("#reset-password-error-link").text('');
								$("#reset-password-error-link").css('display', 'none');
							}
						} else {
							alert(data.error ? data.error : data);
							history.back();
						}
					}
				},
				error: function(jqXHR, textStatus, errorThrown) {
					showLoading(false);
					if (typeof showError === 'function') {
						showError($('#email'), errorThrown ? errorThrown : "Unknown Error");
						$("#reset-password-error-link").text('');
						$("#reset-password-error-link").css('display', 'none');
					} else {
						alert(errorThrown ? errorThrown : "Unknown Error");
						history.back();
					}					
				}
			});
		}
		
		function loginGoogleWithId(googleId, email) {
			showLoading(true);
			var data = {'googleIdToken':googleId};
			if ($('#remember').prop("checked")) {
				data.remember = 'on';
			}
			
			var lastDomain = Cookies.get('workspace');
			if (lastDomain) {
				data.lastDomain = lastDomain;
			}
			var lastWorkspaceId = Cookies.get('workspaceId');
			if (lastWorkspaceId) {
				data.lastWorkspaceId = lastWorkspaceId;
			}
			var lastWorkspaceVersion = Cookies.get('workspaceVersion');
			if (lastWorkspaceVersion) {
				data.lastWorkspaceVersion = lastWorkspaceVersion;
			}
			data.email = email;
			$.post('/login', data)
			.done(function(data) {
				var idToken = googleId;
				var workspace = data.domain;
				var workspaceId = data.workspaceId;
				var data = {'login-google': idToken};
				
				//if (window.location.hash && window.location.hash.length > 0) {
				//	data.hash = window.location.hash.substring(1);
				//}
				if ($('#remember').prop("checked")) {
					data.keeplogin = 'on';
				}
				loginVPository(workspace,	workspaceId, data);
			}).fail(
				function(data) {
					showLoading(false);
					if (data.status == 404) {
						$('#error').html("Account not found, do you want to <a href='subscribe.jsp" + "'>sign up</a>?<br>(Former VPository user? Log in <a href=\"javascript:;\" onclick=\"loginOldVpository()\">here</a>)");
						$("#reset-password-error-link").text('');
						$("#reset-password-error-link").css('display', 'none');
					} else {
						showError($('#email'), data.responseText);
						$("#reset-password-error-link").text('');
						$("#reset-password-error-link").css('display', 'none');
					}
				});
		}
	</script>
